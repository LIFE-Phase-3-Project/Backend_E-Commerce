{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Information",
        "System": "Error"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
   // "DevConnection": "Server=localhost;Database=ECommerce-Life;User Id=sa;Password=Strong(!)Password;TrustServerCertificate=True;MultipleActiveResultSets=True;integrated security=false;"
    //"DevConnection": "Server=RITK-NBN156;Database=Ecommerce;User Id=shpend;Password=Securepass1;MultipleActiveResultSets=true;TrustServerCertificate=True"

     "DevConnection": "${ConnectionStrings__DevConnection}"

    //"DevConnection": "Server=localhost;Database=ECommerce-Life;User Id=sa;Password=Strong(!)Password;TrustServerCertificate=True;MultipleActiveResultSets=True;integrated security=false;"
  },
  "Stripe": {
    "SecretKey": "${Stripe__SecretKey}",
    "WebhookKey": "${Stripe__WebhookKey}"
  },
  "Urls": {
    "SuccessUrl": "http://localhost:8080/success",
    "CancelUrl": "http://localhost:8080/cancel"
  },
  "ElasticSearch": {
    "Uri": "${ElasticSearch__Uri}",
    "CloudId": "${ElasticSearch__CloudId}",
    "Password": "${ElasticSearch__Password}",
    "ApiKey": "${ElasticSearch__ApiKey}"
  },
  "Redis": {
    "ConnectionString": "${Redis__ConnectionString}"
  },
  "Auth0": {
    "Domain": "${Auth0__Domain}",
    "Audience": "${Auth0__Audience}",
    "ClientId": "${Auth0__ClientId}",
    "ClientSecret": "${Auth0__ClientSecret}",
    "Namespace": "${Auth0__Namespace}"
  }
}